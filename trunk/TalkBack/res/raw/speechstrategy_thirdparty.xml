      <ss:packageName>com.adao.android.afm</ss:packageName>
      <ss:currentItemIndex>6</ss:currentItemIndex>
    </ss:filter>
    <ss:formatter>
      <ss:template>@com.google.android.marvin.talkback:string/value_file_manager_back</ss:template>
