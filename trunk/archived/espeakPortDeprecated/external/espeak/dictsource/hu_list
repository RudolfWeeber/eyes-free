// Articles
a         $u     // the
az        $u
aki       $u+   // that

ez        $u+   // this


// Pronouns

// If these are only used for emphasis then then don't need $u
// én        $u
// te        $u
// ő         $u
// mi        $u
// ti        $u
// ők        $u
// maga      $u
// maguk     $u


// Conjunctions
és        $u  $strend2 // and
de        $u+ $pause  // but
vagy      $u+ $pause  // or

mert      $u $pause     // because
miatt     $u           // because
bár       $u $pause     // although
habár     $u $pause     // although

ha        $u $pause  // if
noha      $u $pause  // even thought


// Prepositions etc.


// Negative
nem       $alt2 $strend
ne        $u+

e         $u          // "-e" question
e         $atend      // stressed at end of sentence

