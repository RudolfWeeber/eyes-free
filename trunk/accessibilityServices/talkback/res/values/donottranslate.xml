    <!-- Message template for TalkBack crash reports
    %1$s - Time stamp of crash
    %2$s - Android SDK Version Code
    %3$s - Android Device Details
    %4$s - TalkBack Version Code
    %5$s - Stack trace
    %6$s - Last processed accessibility event -->
    <string name="template_crash_report_message">TalkBack crash report generated on: %1$s\n\nAndroid OS Version: %2$s\n
      Device Details: %3$s\nTalkBack Version: %4$s\n\nStack Trace:\n%5$s\nLast Accessibility Event:\n%6$s\n\n</string>

