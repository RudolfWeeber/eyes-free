    <!-- Volume manager -->

    <!--
      Source: An utterance template for announcing the current volume for a particular stream (e.g. music, alarm, etc) before changes are made.
      Description: This is spoken to the user. [CHAR LIMIT="NONE"]
    -->
    <string name="template_stream_volume"><xliff:g id="stream_name">%s</xliff:g> volume <xliff:g id="percent">%d</xliff:g> percent.</string>

    <!--
      Source: An utterance template for announcing the current volume for a particular stream (e.g. music, alarm, etc) after changes have been made.
      Description: This is spoken to the user. [CHAR LIMIT="NONE"]
    -->
    <string name="template_stream_volume_set"><xliff:g id="stream_name">%s</xliff:g> volume set to <xliff:g id="percent">%d</xliff:g> percent.</string>

    <!--
      Source: A value for announcing the "master" volume stream.
      Description: This is spoken to the user. [CHAR LIMIT="NONE"]
    -->
    <string name="value_stream_master">Master</string>

    <!--
      Source: A value for announcing the "voice call" volume stream.
      Description: This is spoken to the user. [CHAR LIMIT="NONE"]
