<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- VALUES for the Google search box -->

    <!--
      Source: Description for icon that changes search category.
      Description: This is spoken to the user. [CHAR LIMIT="NONE"]
    -->
    <string name="template_change_search_category">Change search category</string>
    <!--
      Source: Description for Google search box widget.
      Description: This is spoken to the user. [CHAR LIMIT="NONE"]
