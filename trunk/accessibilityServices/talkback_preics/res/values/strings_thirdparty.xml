    <!-- This file contains strings for third-party apps. -->


    <!-- Amazon app -->

    <!--
      Source: A value for announcing the "Go" button in the Amazon application.
      Description: This is spoken to the user. [CHAR LIMIT="NONE"]
    -->
    <string name="value_amazon_go">Go.</string>

    <!--
      Source: A value for announcing the "Search" button in the Amazon application.
      Description: This is spoken to the user. [CHAR LIMIT="NONE"]
    -->
    <string name="value_amazon_search">Search.</string>

    <!--
      Source: An utterance template for announcing the number of items in the shopping cart in the Amazon application.
      Description: This is spoken to the user. [CHAR LIMIT="NONE"]
    -->
    <string name="template_amazon_quantity">Quantity <xliff:g id="count">%1$s</xliff:g></string>


    <!-- Amazon mp3 app -->

    <!--
      Source: An utterance template for announcing a "Buy" button in the Amazon MP3 application.
      Description: This is spoken to the user. [CHAR LIMIT="NONE"]
    -->
    <!-- %1$s - The text of the buy button, indicates the price; -->
    <string name="template_amazon_mp3_buy_button">Buy button, <xliff:g id="title">%1$s</xliff:g></string>


