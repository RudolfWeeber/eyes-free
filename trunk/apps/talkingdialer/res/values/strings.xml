<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!--
      Source: Activity title.
      Description: Title for the phone book, for browsing contacts.
    -->
    <string name="phonebook">Phone book</string>
    <!--
      Source: Activity title.
      Description: Title for the video contacts list for browsing video call.
    -->
    <string name="video_contacts">Video Contacts</string>
    <!--
      Source: Activity title.
      Description: Title for the phone dialer, for dialing numbers.
    -->
    <string name="dialing_mode">Dialing mode</string>
    <!--
      Source: Application name.
      Description: This application provides a phone dialer for blind users that speaks numbers aloud.
    -->
    <string name="app_name">Talking Dialer</string>
    <!--
      Source: Button label.
      Description: Calls the current phone number or contact.
    -->
    <string name="call">Call</string>
    <!--
      Source: Spoken feedback.
      Description: Confirmation that a character was deleted.
    -->
    <string name="deleted">%s deleted.</string>
    <!--
      Source: Spoken feedback.
      Description: Spoken when no characters are left to delete after pressing backspace.
    -->
    <string name="nothing_to_delete">Nothing to delete</string>
    <!--
      Source: Spoken feedback.
      Description: Confirmation that the user is about to dial a phone number.
    -->
    <string name="you_are_about_to_dial">You are about to dial <xliff:g id="phone_number">%s</xliff:g>.</string>
    <!--
      Source: Spoken feedback.
      Description: Confirmation that the user is about to video dial a gchat contact.
    -->
    <string name="you_are_about_to_video_call">You are about to video call <xliff:g id="email_address">%s</xliff:g>.</string>
    <!--
      Source: Spoken feedback.
      Description: Confirmation that the user has selected a contact.
    -->
    <string name="you_have_selected">You have selected <xliff:g id="contact_name">%s</xliff:g>.</string>
    <!--
      Source: Label for phone number location.
