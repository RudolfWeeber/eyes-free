    -->
    <string name="cell">cell</string>
    <!--
      Source: Label for phone number location.
      Description: A work phone number.
    -->
    <string name="work">work</string>
     <!--
      Source: Label for phone number location.
      Description: A default phone number.
    -->
    <string name="phone">phone</string>
